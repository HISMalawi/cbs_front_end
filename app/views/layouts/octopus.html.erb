<!doctype html>
<html class="fixed">
<head>

  <!-- Basic -->
  <meta charset="UTF-8">

  <title>CBS</title>
  <meta name="keywords" content="Baobab Health" />
  <meta name="description" content="CBS - Baobab Health">
  <meta name="author" content="baobab developers">

  <!-- Mobile Metas -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

  <!-- Web Fonts  -->
  <%= stylesheet_link_tag 'octopus/google-web-fonts', rel: 'stylesheet' %>
  <!-- Vendor CSS -->
  <%= stylesheet_link_tag 'octopus/bootstrap', rel: 'stylesheet' %>
  <%= stylesheet_link_tag 'octopus/font-awesome', rel: 'stylesheet' %>
  <%= stylesheet_link_tag 'octopus/magnific-popup', rel: 'stylesheet' %>
  <%= stylesheet_link_tag 'octopus/datepicker3', rel: 'stylesheet' %>

  <!-- Theme CSS -->
  <%= stylesheet_link_tag 'octopus/theme', rel: 'stylesheet' %>

  <!-- Skin CSS -->
  <%= stylesheet_link_tag 'octopus/default', rel: 'stylesheet' %>

  <!-- Theme Custom CSS -->
  <%= stylesheet_link_tag 'octopus/theme-custom' %>

  <!-- Head Libs -->
  <%= javascript_include_tag 'octopus/modernizr' %>

</head>
<body>
<section class="body">

  <!-- start: header -->
